#import "Category.h"

@implementation Category
@synthesize catId = _catId;
@synthesize name = _name;
@synthesize subCategorys = _subCategorys;
@synthesize imageUrl =_imageUrl;
- (void)dealloc
{
  [_imageUrl release];
    [_name release];
    [_subCategorys release];
    [super dealloc];
}

- (id)initWithAttributes:(NSDictionary *)attributes
{
  if(self=[super init])
  {
    
    self.imageUrl =[NSString stringWithFormat:@"%@%@", kWebSiteURL, [attributes valueForKey:@"img"]];
    self.name =[attributes objectForKey:@"name"];
    self.catId =[[attributes objectForKey:@"type_id"] integerValue];
  }
  return self;
}

@end
