#import "Model.h"

@implementation Model
@synthesize modelId = _modelId;
@synthesize imgURL = _imgURL;
@synthesize type = _type;

- (void)dealloc
{
    [_modelId release];
    [_imgURL release];
    [super dealloc];
}

- (id)initWithAttributes:(NSDictionary *)attributes
{
    if(self = [super init]){
        self.modelId = [NSString stringWithFormat:@"%@",[attributes valueForKey:@"model_id"]];
        self.imgURL = [NSString stringWithFormat:@"%@%@",kWebSiteURL,[attributes valueForKey:@"model_img"]];
        self.type = SystemType;
    }
    
    return self;
}


@end
