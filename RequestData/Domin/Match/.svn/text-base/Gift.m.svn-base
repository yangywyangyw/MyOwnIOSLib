#import "Gift.h"

@implementation Gift
@synthesize giftId =_giftId;
@synthesize giftName =_giftName;
@synthesize giftImg =_giftImg;
@synthesize thumbsImg =_thumbsImg;
@synthesize needCoin =_needCoin;
@synthesize giftIntro =_giftIntro;
@synthesize giftBrand =_giftBrand;

-(void)dealloc
{
  [_giftName release];
  [_giftImg release];
  [_thumbsImg release];
  [_giftIntro release];
  [_giftBrand release];
  [super dealloc];
}

-(id)initWithAttributes:(NSDictionary *)attributes
{
  if(self =[super init])
  {
    self.giftId =[[attributes objectForKey:@"gift_id"] integerValue];
    self.giftName =[attributes objectForKey:@"name"];
    self.giftImg =[NSString stringWithFormat:@"%@%@", kWebSiteURL, [attributes valueForKey:@"img"]];
    self.thumbsImg =[NSString stringWithFormat:@"%@%@", kWebSiteURL, [attributes valueForKey:@"thumbs"]];
    self.needCoin =[[attributes objectForKey:@"coin"] integerValue];
    self.giftIntro =[attributes objectForKey:@"intro"];
    self.giftBrand =[attributes objectForKey:@"brand"];
    
  }
  return self;
}

@end
