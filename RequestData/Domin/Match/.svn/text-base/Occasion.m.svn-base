#import "Occasion.h"

@implementation Occasion
@synthesize situationId=_situationId;
@synthesize name=_name;
@synthesize thumlURL=_thumlURL;
@synthesize imgURL=_imgURL;
@synthesize station = _station;

-(void)dealloc
{
    [_name release];
    [_thumlURL release];
    [_imgURL  release];
    [super dealloc];
}

-(id)initWithAttributes:(NSDictionary *)attributes
{
    if(self=[super init]){
        
        self.situationId=[[attributes valueForKey:@"situation_id"] integerValue];
        self.name=[NSString stringWithFormat:@"%@",[attributes valueForKey:@"name"]];
        self.thumlURL=[NSString stringWithFormat:@"%@%@",kWebSiteURL,[attributes valueForKey:@"thumbs"]];
        self.imgURL=[NSString stringWithFormat:@"%@%@",kWebSiteURL,[attributes valueForKey:@"img"]];
        self.station = YES;
    }
    
    return self;
}



@end
