#import "Store.h"

@implementation Store
@synthesize storeId=_storeId;
@synthesize name=_name;
@synthesize address=_address;
@synthesize phone = _phone;
@synthesize buyURL=_buyURL;
@synthesize latitude=_latitude;
@synthesize longtitude=_longtitude;

-(void)dealloc
{
    [_name release];
    [_address release];
    [_buyURL release];
    [_latitude release];
    [_longtitude release];
    [super dealloc];
}

-(id)initWithAttributes:(NSDictionary *)attributes
{
    if(self=[super init]){
        
        self.storeId=[[attributes valueForKey:@"store_id"] integerValue];
        self.name=[NSString stringWithFormat:@"%@",[attributes valueForKey:@"name"]];
        self.address=[NSString stringWithFormat:@"%@",[attributes valueForKey:@"address"]];
        self.buyURL=[NSString stringWithFormat:@"%@",[attributes valueForKey:@"show_url"]];
        self.latitude=[NSString stringWithFormat:@"%@",[attributes valueForKey:@"geo_latitude"]];
        self.longtitude=[NSString stringWithFormat:@"%@",[attributes valueForKey:@"geo_longitude"]];
    }
    
    return self;
}

@end
