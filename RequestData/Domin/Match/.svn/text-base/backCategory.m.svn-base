#import "backCategory.h"
#import "Utils.h"
@implementation backCategory
@synthesize urlStirng =_urlStirng;
@synthesize name =_name;
@synthesize typeId =_typeId;

-(void)dealloc
{
  [_urlStirng release];
  [_name release];
  [super dealloc];
}
- (id)initWithAttributes:(NSDictionary *)attributes
{
  if(self = [super init]){
    self.name = [NSString stringWithFormat:@"%@",[attributes valueForKey:@"name"]];
     self.urlStirng = [NSString stringWithFormat:@"%@%@",kWebSiteURL,[attributes valueForKey:@"img"]];
    self.typeId =[[attributes valueForKey:@"type_id"]intValue];
   }
  
  return self;
}

@end
